package helloword;

import java.util.Scanner;

public class hello {
	public static void main(String[] args) {
		int classNum = 2;
		int classMem =3;
		int avg=0;
		int sum=0;
		Scanner input = new Scanner(System.in);
		int score = input.nextInt();
		for(int i=0; i<classNum;i++){
			for(int j=0;j<classMem;j++){
				sum += score;
			}
		}
		
	}
}
