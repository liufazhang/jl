package com.imooc;

public class Initial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Animal obj1 = new Animal();
		Animal obj2 = new Dog();//父类的引用可以指向子类
		//Dog obj3 = new Animal();错
		obj1.eat();
		obj2.eat();
		Animal obj3 = new Cat();
		obj3.eat();
		
		Dog dog =new Dog();
		Animal animal = dog;//自动类型提升，向上类型转换
	}

}
